{
    "manifest_version": 3,
    "name": "Versusia Genesis Bar Display",
    "version": "1.0",
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "notifications"
    ],
    "action": {
        "default_icon": {
          "16": "icon16.png",
          "32": "icon32.png",
          "48": "icon48.png"
        }
    },
    "externally_connectable": {
            "matches": [
            "*://game.granbluefantasy.jp/*"
        ]
    },
        "icons": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
    }
}